{
  "name": "crudth/reviewa",
  "license": "MIT",
  "description": "Application Reviews",
  "require": {
    "php": "^7.4",
    "ext-pdo": "*",
    "ext-dom": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-ctype": "*",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "ext-memcached": "*",
    "cocur/slugify": "^4.0",
    "amstaffix/pagination": "^1.1",
    "claviska/simpleimage": "^3.5",
    "mobiledetect/mobiledetectlib": "^2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "9"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "System\\": "system/"
    },
    "files": [
      "system/Helpers/functions.php",
      "app/Support/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
